  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.3",
    "@cloudflare/workers-types": "^4.20241018.0",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/web-push": "^3.6.3",
    "typescript": "^5.5.3",
    "wrangler": "^3.80.0"
  }
}
